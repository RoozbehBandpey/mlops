{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mlopsadf1"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"excelonoutput_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'excelonoutput'"
		},
		"execloninput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'execloninput'"
		},
		"exinput1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'exinput1'"
		},
		"milwaukeetoolsdata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'milwaukeetoolsdata'"
		},
		"mltoolsinput_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'mltoolsinput'"
		},
		"vijaiblobstore_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'vijaiblobstore'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mlopssa.dfs.core.windows.net"
		},
		"excelonoutput_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mlopssa.dfs.core.windows.net"
		},
		"milwaukeetoolsdata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://milwaukeetoolsdata.dfs.core.windows.net"
		},
		"mltoolsinput_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://diemodeltraining.dfs.core.windows.net"
		},
		"vijaiblobstore_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vkannlabelinge2eod.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"excelonoutput_accountKey": {
						"value": "[parameters('excelonoutput_accountKey')]"
					},
					"execloninput_connectionString": {
						"value": "[parameters('execloninput_connectionString')]"
					},
					"exinput1_connectionString": {
						"value": "[parameters('exinput1_connectionString')]"
					},
					"milwaukeetoolsdata_accountKey": {
						"value": "[parameters('milwaukeetoolsdata_accountKey')]"
					},
					"mltoolsinput_accountKey": {
						"value": "[parameters('mltoolsinput_accountKey')]"
					},
					"vijaiblobstore_accountKey": {
						"value": "[parameters('vijaiblobstore_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"excelonoutput_properties_typeProperties_url": {
						"value": "[parameters('excelonoutput_properties_typeProperties_url')]"
					},
					"milwaukeetoolsdata_properties_typeProperties_url": {
						"value": "[parameters('milwaukeetoolsdata_properties_typeProperties_url')]"
					},
					"mltoolsinput_properties_typeProperties_url": {
						"value": "[parameters('mltoolsinput_properties_typeProperties_url')]"
					},
					"vijaiblobstore_properties_typeProperties_url": {
						"value": "[parameters('vijaiblobstore_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}