{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mlopsadf1"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"excelonoutput_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'excelonoutput'"
		},
		"execloninput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'execloninput'"
		},
		"exinput1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'exinput1'"
		},
		"milwaukeetoolsdata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'milwaukeetoolsdata'"
		},
		"mlopssa_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'mlopssa'"
		},
		"mltoolsinput_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'mltoolsinput'"
		},
		"vijaiblobstore_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'vijaiblobstore'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mlopssa.dfs.core.windows.net"
		},
		"cocozipurl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://images.cocodataset.org/zips/train2017.zip"
		},
		"excelonoutput_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mlopssa.dfs.core.windows.net"
		},
		"milwaukeetoolsdata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://milwaukeetoolsdata.dfs.core.windows.net"
		},
		"mlopssa_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mlopssa.dfs.core.windows.net"
		},
		"mltoolsinput_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://diemodeltraining.dfs.core.windows.net"
		},
		"vijaiblobstore_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://vkannlabelinge2eod.dfs.core.windows.net"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/cocozipurl')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "HttpServer",
				"typeProperties": {
					"url": "[parameters('cocozipurl_properties_typeProperties_url')]",
					"enableServerCertificateValidation": false,
					"authenticationType": "Anonymous"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/excelonoutput')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
